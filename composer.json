{
    "name": "laravie/kurenai",
    "description": "Markdown document parser with metadata.",
    "keywords": ["kurenai", "markdown", "metadata", "document"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dayle Rees",
            "email": "theppunkfan@gmail.com",
            "homepage": "https://github.com/daylerees"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kurenai\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/yaml": "^3.0 || ^4.0"
    },
    "require-dev": {
        "erusev/parsedown-extra": "^1.7.1",
        "erusev/parsedown-extra": "~0.7",
        "league/commonmark": "~0.17",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "conflict": {
        "daylerees/kurenai": "~1.0"
    },
    "suggest": {
        "erusev/parsedown": "Parsing markdown using erusev/parsedown (^1.7.1).",
        "erusev/parsedown-extra": "Parsing markdown using erusev/parsedown-extra (~0.7).",
        "league/commonmark": "Parsing markdown using league/commonmark (~0.17)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability" : "dev"
}
